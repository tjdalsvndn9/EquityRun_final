{
  "name": "reacttest",
  "version": "0.1.0",
  "proxy": {
    "/auth/*": {
      "target": "http://localhost:5000"
    },
    "/api/*": {
      "target": "http://localhost:5000"
    },
    "/job":{
      "target": "http://localhost:5000"
    }
  },
  "private": true,
  "dependencies": {
    "@redux-offline/redux-offline": "^2.2.1",
    "bootstrap": "^4.0.0",
    "chart.js": "^2.7.1",
    "fetch-mock": "^5.13.1",
    "history": "^4.7.2",
    "moment": "^2.20.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "rcolor": "^1.0.1",
    "react": "^16.0.0",
    "react-accessible-accordion": "^2.0.0",
    "react-chartjs-2": "^2.7.0",
    "react-dom": "^16.0.0",
    "react-html5video": "^2.5.1",
    "react-https-redirect": "^1.0.9",
    "react-infinite-scroller": "^1.1.3",
    "react-pdf": "^2.5.3",
    "react-player": "^1.1.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.0.14",
    "react-slick": "^0.18.1",
    "react-tag-autocomplete": "^5.5.0",
    "react-test-renderer": "^16.0.0",
    "react-waypoint": "^8.0.0",
    "reactstrap": "^5.0.0-alpha.4",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-mock-store": "^1.3.0",
    "redux-observable": "^0.17.0",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.5.2",
    "semantic-ui-react": "^0.75.0",
    "slick-carousel": "^1.8.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start && start",
    "start": "npm-run-all watch-css start-js",
    "build": "npm build build-css && react-scripts ",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.14.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fetch-mock": "^5.13.1",
    "jest": "^21.2.1",
    "jest-fetch-mock": "^1.4.1",
    "jest-mock-axios": "^1.0.21",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "redux-mock-store": "^1.5.1",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ]
  }
}
